{
  "name": "andreuramos/accounting-api",
  "description": "Backend for an accounting app ",
  "license": "MIT",
  "config": {
    "platform": {
      "php": "8.1.8"
    }
  },
  "require": {
    "symfony/routing": "^6.1",
    "symfony/http-foundation": "^6.1",
    "symfony/messenger": "^6.1",
    "php-di/php-di": "^6.4",
    "robmorgan/phinx": "^0.13.4",
    "vlucas/phpdotenv": "^5.5",
    "firebase/php-jwt": "^6.4",
    "symfony/console": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "guzzlehttp/guzzle": "^7.5",
    "larapack/dd": "^1.1",
    "phpspec/prophecy": "^1.15",
    "phpspec/prophecy-phpunit": "^2.0",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "App\\" : "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests"
    }
  }
}
